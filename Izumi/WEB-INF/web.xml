<web-app>
	<servlet>
		<servlet-name>UserServlet</servlet-name>
		<servlet-class>User.Regist.UserServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UserServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/helshin</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>login</servlet-name>
		<jsp-file>/WEB-INF/jsp/user/helshin2.jsp</jsp-file>
	</servlet>
	
	<servlet>
		<servlet-name>FrontServlet</servlet-name>
		<servlet-class>Kanrisya.FrontServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FrontServlet</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>/WEB-INF/jsp/kanrisya/kanrisyamain.jsp</welcome-file>
	</welcome-file-list>
	
	<servlet-mapping>
  		<servlet-name>default</servlet-name>
  		<url-pattern>/Browser/*</url-pattern>
	</servlet-mapping>
</web-app>