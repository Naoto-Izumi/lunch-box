<web-app>
	<servlet>
		<servlet-name>UserServlet</servlet-name>
		<servlet-class>User.Regist.UserServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UserServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/helshin</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>login</servlet-name>
		<jsp-file>/WEB-INF/jsp/user/helshin2.jsp</jsp-file>
	</servlet>
	
	<servlet>
		<servlet-name>UriageServlet</servlet-name>
		<servlet-class>Kanrisya.Uriage.UriageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UriageServlet</servlet-name>
		<url-pattern>/uriage</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>uriage</servlet-name>
		<url-pattern>/helshin2</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>uriage</servlet-name>
		<jsp-file>/kanrisya.jsp</jsp-file>
	</servlet>
	
	<servlet>
		<servlet-name>FrontServlet</servlet-name>
		<servlet-class>Kanrisya.Lock.FrontServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FrontServlet</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>/WEB-INF/jsp/kanrisya/kanrisya.jsp</welcome-file>
	</welcome-file-list>
	
	<servlet-mapping>
  		<servlet-name>default</servlet-name>
  		<url-pattern>/css/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
  		<servlet-name>default</servlet-name>
  		<url-pattern>/js/*</url-pattern>
	</servlet-mapping>
</web-app>